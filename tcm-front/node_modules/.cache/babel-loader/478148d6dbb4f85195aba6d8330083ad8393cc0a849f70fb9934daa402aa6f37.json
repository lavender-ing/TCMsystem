true>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"price\"\r\n          label=\"原价\"\r\n          width=\"100px\"\r\n          :show-overflow-tooltip=true>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"discount\"\r\n          label=\"折扣价\"\r\n          width=\"100px\"\r\n          :show-overflow-tooltip=true>\r\n          <template slot-scope=\"scope\">{{scope.row.discount*scope.row.price}}</template>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          label=\"图片\"\r\n          prop=\"picturepath\"\r\n          width=\"100px\"\r\n          :show-overflow-tooltip=true>\r\n          <template slot-scope=\"scope\">\r\n            <el-image\r\n              v-if=\"scope.row.picturepath\"\r\n              @click=\"\r\n                bigImg(`http://localhost:8081/`+scope.row.picturepath)\"\r\n              style=\"width: 50px; height: 50px; margin-bottom:-4px\"\r\n              :src=\"`http://localhost:8081/`+scope.row.picturepath\"\r\n              :preview-src-list=\"srcList\">\r\n            </el-image>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"number\"\r\n          label=\"数量\"\r\n          width=\"100px\"\r\n          :show-overflow-tooltip=true>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          label=\"总价\"\r\n          width=\"100px\"\r\n          :show-overflow-tooltip=true>\r\n          <template slot-scope=\"scope\">\r\n            {{scope.row.price*scope.row.discount*scope.row.number}}\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"primary\"\r\n              @click=\"handleOrder(scope.$index, scope.row)\">提交订单</el-button>\r\n\r\n            <el-button\r\n              size=\"mini\"\r\n              @click=\"handleNumber(scope.$index, scope.row)\">修改数量</el-button>\r\n          </template>\r\n        </el-table-column>\r\n\r\n      </el-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      srcList:['']\r\n    }\r\n  },\r\n  created(){\r\n    this.loadData()\r\n  },\r\n  methods: {\r\n      //加载信息\r\n      loadData(){\r\n        this.$axios.get('/cart/getall/'+this.$store.state.user.id)\r\n        .then(res=>{\r\n          if(res.data.code==200){\r\n            this.tableData=res.data.result\r\n          }else{\r\n            this.$alert(res.data.message, '提示', {\r\n                confirmButtonText: '确定'\r\n            })\r\n          }\r\n        })\r\n      },\r\n      //提交订单\r\n      handleOrder(index, row) {\r\n        this.$router.push({\r\n          name:'cartorder',\r\n          query:{\r\n            data:JSON.stringify(row)\r\n          }\r\n        })\r\n      },\r\n      //详情\r\n      handleDetail(index, row) {\r\n        this.$router.push({\r\n          name:'commoditydetails',\r\n          query:{\r\n            data:JSON.stringify(row)\r\n          }\r\n        })\r\n      },\r\n      //大图\r\n      bigImg(url) {\r\n        this.srcList[0] = url;\r\n      },\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.table{\r\n  margin:20px;\r\n}\r\n\r\n.btn{\r\n  margin: 0px;\r\n  margin-bottom: 20px;\r\n  margin-right: 20px;\r\n  float: right;\r\n}\r\n\r\n</style>"],"mappings":";AA4FA;EACAA,KAAA;IACA;MACAC,SAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,QAAA;EACA;EACAC,OAAA;IACA;IACAD,SAAA;MACA,KAAAE,MAAA,CAAAC,GAAA,wBAAAC,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAC,EAAA,EACAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAb,IAAA,CAAAc,IAAA;UACA,KAAAb,SAAA,GAAAY,GAAA,CAAAb,IAAA,CAAAe,MAAA;QACA;UACA,KAAAC,MAAA,CAAAH,GAAA,CAAAb,IAAA,CAAAiB,OAAA;YACAC,iBAAA;UACA;QACA;MACA;IACA;IACA;IACAC,YAAAC,KAAA,EAAAC,GAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QACAC,IAAA;QACAC,KAAA;UACAzB,IAAA,EAAA0B,IAAA,CAAAC,SAAA,CAAA