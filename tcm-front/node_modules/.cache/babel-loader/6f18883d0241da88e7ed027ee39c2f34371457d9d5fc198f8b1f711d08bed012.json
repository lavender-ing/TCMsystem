/template>\r\n        {{this.tableData.type}}\r\n      </el-descriptions-item>\r\n\r\n      <el-descriptions-item>\r\n        <template slot=\"label\">\r\n          描述\r\n        </template>\r\n        {{this.tableData.description}}\r\n      </el-descriptions-item>\r\n\r\n      <el-descriptions-item>\r\n        <template slot=\"label\">\r\n          图片\r\n        </template>\r\n        <el-image\r\n          v-if=\"this.tableData.picturepath\"\r\n          @click=\"\r\n            bigImg()\"\r\n          style=\"width: 50px; height: 50px; margin-bottom:-4px\"\r\n          :src=\"`http://localhost:8081/`+this.tableData.picturepath\"\r\n          :preview-src-list=\"srcList\">\r\n        </el-image>\r\n      </el-descriptions-item>\r\n\r\n    </el-descriptions>\r\n\r\n    <el-descriptions class=\"margin-top\" title=\"商品评论\" :column=\"1\" :border=\"true\" :data=\"commentList\" labelStyle=\"width:25%;\">\r\n\r\n      <el-descriptions-item>\r\n        <template slot=\"label\">\r\n          原价\r\n        </template>\r\n        {{this.tableData.price}}\r\n      </el-descriptions-item>\r\n\r\n      <el-descriptions-item>\r\n        <template slot=\"label\">\r\n          描述\r\n        </template>\r\n        {{this.tableData.description}}\r\n      </el-descriptions-item>\r\n\r\n    </el-descriptions>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      srcList:[''],\r\n      commentList:[],\r\n    }\r\n  },\r\n  created(){\r\n    this.tableData=JSON.parse(this.$route.query.data)\r\n  },\r\n  methods:{\r\n    goBack(){\r\n      this.$router.back()\r\n    },\r\n    //大图\r\n    bigImg() {\r\n      this.srcList[0] = 'http://localhost:8081/'+this.tableData.picturepath\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.el-descriptions{\r\n  margin:20px;\r\n}\r\n</style>"],"mappings":"AA6FA;EACAA,KAAA;IACA;MACAC,SAAA;MACAC,OAAA;MACAC,WAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAH,SAAA,GAAAI,IAAA,CAAAC,KAAA,