uter.back()\r\n    },\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.$axios.post('/advertisement/insert',{\r\n            description:this.ruleForm.description,\r\n            picturepath:this.ruleForm.picturepath,\r\n          }).then(res=>{\r\n            if(res.data.code==200){\r\n              this.$message({\r\n                type: 'success',\r\n                message: \"添加成功！\",\r\n              });\r\n            }else{\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: 'error',\r\n            message: \"请检查输入信息！\",\r\n          });\r\n        }\r\n      });\r\n    },\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n    //图片上传\r\n    handleAvatarSuccess(res, file) {\r\n      this.ruleForm.picturepath=res.result\r\n    },\r\n\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === 'image/jpeg';\r\n      const isLt2M = file.size / 1024 / 1024 < 4;\r\n\r\n      if (!isJPG) {\r\n        this.$message.error('上传广告图片只能是 JPG 格式!');\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error('上传广告图片大小不能超过 4MB!');\r\n      }\r\n      return isJPG && isLt2M;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.el-form{\r\n  margin: auto;\r\n  margin-top:20px;\r\n  width:50%;\r\n}\r\n\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409EFF;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n\r\n</style>"],"mappings":"AAiCA;EACAA,KAAA;IACA;MACAC,QAAA;QACAC,WAAA;QACAC,WAAA;MACA;MACAC,KAAA;QACAF,WAAA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAJ,WAAA;UAAAE,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;MACAC,KAAA;IACA;EACA;EACAC,QAAA,GAEA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACAC,WAAAC,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAC,MAAA,CAAAC,IAAA;YACAlB,WAAA,OAAAD,QAAA,CAAAC,WAAA;YACAC,WAAA,OAAAF,QAAA,CAAAE;UACA,GAAAkB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAtB,IAAA,CAAAuB,IAAA;cACA,KAAAC,QAAA;gBACAC,IAAA;gBACAnB,OAAA;cACA;YACA;cACA,KAAAkB,QAAA;gBACAC,IAAA;gBACAnB,OAAA,EAAAgB,GAAA,CAAAtB,IAAA,CAAAM;cACA;YACA;UACA;QACA;UACA,KAAAkB,QAAA;YACAC,IAAA;YACAnB,OAAA;UACA;QACA;MACA;IACA;IACAoB,UAAAX,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAY,WAAA;IACA;IACA;IACAC,oBAAAN,GAAA,EAAAO,IAAA;MACA,KAAA5B,QAAA,CAAAE,WAAA,GAAAmB,GAAA,CAAAQ,MAAA;IACA;IAEAC,mBAAAF,IAAA;MACA,MAAAG,KAAA,GAAAH,IAAA,CAAAJ,IAAA;MACA,MAAAQ,MAAA,GAAAJ,IAAA,CAAAK,IAAA;MAEA,KAAAF,KAAA;QACA,KAAAR,QAAA,CAAAW,KAAA;MACA;MACA,KAAAF,MAAA;QACA,KAAAT,QAAA,CAAAW,KAAA;MACA;MACA,OAAAH,KAAA,IAAAC,MAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}