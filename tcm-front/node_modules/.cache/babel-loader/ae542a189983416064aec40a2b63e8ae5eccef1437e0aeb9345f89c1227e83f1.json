{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      tableData: []\n    };\n  },\n  created() {\n    this.loadData();\n  },\n  methods: {\n    //加载信息\n    loadData() {\n      this.$axios.get('/order/getbyuid/' + this.$store.state.user.id).then(res => {\n        if (res.data.code == 200) {\n          this.tableData = res.data.result;\n        } else {\n          this.$alert(res.data.message, '提示', {\n            confirmButtonText: '确定'\n          });\n        }\n      });\n    },\n    //支付\n    handlePay(index, row) {\n      this.$confirm('是否支付订单?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(res => {\n        this.$axios.put('/order/updatestate', {\n          oid: row.oid,\n          state: '已支付'\n        }).then(res => {\n          if (res.data.code == 200) {\n            this.$message({\n              type: 'success',\n              message: '支付成功!'\n            });\n            location.reload();\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.data.message\n            });\n          }\n        });\n      }).catch(fail => {\n        this.$message({\n          type: 'info',\n          message: '已取消'\n        });\n      });\n    },\n    //确认收货\n    handleComfirm(index, row) {\n      this.$confirm('是否确认收货?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(res => {\n        this.$axios.put('/order/updatestate', {\n          oid: row.oid,\n          state: '已签收'\n        }).then(res => {\n          if (res.data.code == 200) {\n            this.$message({\n              type: 'success',\n              message: '签收成功!'\n            });\n            location.reload();\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.data.message\n            });\n          }\n        });\n      }).catch(fail => {\n        this.$message({\n          type: 'info',\n          message: '已取消'\n        });\n      });\n    },\n    //评论\n    handleComment(index, row) {\n      this.$prompt('请输入商品评论', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        inputValidator: value => {\n          return value.length > 0;\n        },\n        inputErrorMessage: '请输入至少一个字符'\n      }).then(({\n        value\n      }) => {\n        console.log(value);\n        this.$axios.put('/order/updatecomment', {\n          oid: row.oid,\n          comment: value\n        }).then(res => {\n          if (res.data.code == 200) {\n            this.$message({\n              type: 'success',\n              message: '评论成功!'\n            });\n            location.reload();\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.data.message\n            });\n          }\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '取消输入'\n        });\n      });\n    },\n    //详情\n    handleDetail(index, row) {\n      this.$router.push({\n        name: 'ordersdetails',\n        query: {\n          data: JSON.stringify(row)\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","tableData","created","loadData","methods","$axios","get","$store","state","user","id","then","res","code","result","$alert","message","confirmButtonText","handlePay","index","row","$confirm","cancelButtonText","type","put","oid","$message","location","reload","catch","fail","handleComfirm","handleComment","$prompt","inputValidator","value","length","inputErrorMessage","console","log","comment","handleDetail","$router","push","name","query","JSON","stringify"],"sources":["src/views/Consumer/Orders/List/index.vue"],"sourcesContent":["<template>\r\n  <div>\r\n\r\n    <div class=\"table\">\r\n      <el-table\r\n        :data=\"tableData\"\r\n        :border=true\r\n        style=\"width: 100%\"\r\n        :stripe=true\r\n        :header-cell-style=\"{'text-align':'center'}\"\r\n        :cell-style=\"{'text-align':'center'}\">\r\n\r\n        <el-table-column\r\n          prop=\"oid\"\r\n          label=\"订单ID\"\r\n          width=\"100px\"\r\n          :show-overflow-tooltip=true>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"commodity.name\"\r\n          label=\"商品\"\r\n          width=\"100px\"\r\n          :show-overflow-tooltip=true>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"commodity.brand\"\r\n          label=\"商品品牌\"\r\n          width=\"100px\"\r\n          :show-overflow-tooltip=true>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"number\"\r\n          label=\"数量\"\r\n          width=\"100px\"\r\n          :show-overflow-tooltip=true>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          label=\"总价\"\r\n          width=\"100px\"\r\n          :show-overflow-tooltip=true>\r\n          <template slot-scope=\"scope\">{{scope.row.number*scope.row.commodity.price*scope.row.commodity.discount}}</template>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"address\"\r\n          label=\"地址\"\r\n          :show-overflow-tooltip=true>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"createtime\"\r\n          label=\"创建时间\"\r\n          :show-overflow-tooltip=true>\r\n          <template slot-scope=\"scope\">{{scope.row.createtime|dateFormat}}</template>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"state\"\r\n          label=\"订单状态\"\r\n          width=\"100px\"\r\n          :show-overflow-tooltip=true>\r\n        </el-table-column>\r\n\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n\r\n            <el-button\r\n            v-if=\"scope.row.state=='未支付'\"\r\n              size=\"mini\"\r\n              type=\"primary\"\r\n              @click=\"handlePay(scope.$index, scope.row)\">支付</el-button>\r\n\r\n            <el-button\r\n              v-if=\"scope.row.state=='已支付'\"\r\n              size=\"mini\"\r\n              @click=\"handleComfirm(scope.$index, scope.row)\">确认收货</el-button>\r\n\r\n            <el-button\r\n              v-if=\"scope.row.state=='已签收'\"\r\n              size=\"mini\"\r\n              @click=\"handleComment(scope.$index, scope.row)\">评论</el-button>\r\n\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"info\"\r\n              @click=\"handleDetail(scope.$index, scope.row)\">详情</el-button>\r\n          </template>\r\n        </el-table-column>\r\n\r\n      </el-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: []\r\n    }\r\n  },\r\n  created(){\r\n    this.loadData()\r\n  },\r\n  methods: {\r\n      //加载信息\r\n      loadData(){\r\n        this.$axios.get('/order/getbyuid/'+this.$store.state.user.id)\r\n        .then(res=>{\r\n          if(res.data.code==200){\r\n            this.tableData=res.data.result\r\n          }else{\r\n            this.$alert(res.data.message, '提示', {\r\n                confirmButtonText: '确定'\r\n            })\r\n          }\r\n        })\r\n      },\r\n      //支付\r\n      handlePay(index, row){\r\n        this.$confirm('是否支付订单?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(res=>{\r\n          this.$axios.put('/order/updatestate',{\r\n            oid:row.oid,\r\n            state:'已支付'\r\n          }).then(res=>{\r\n            if(res.data.code==200){\r\n              this.$message({\r\n                type: 'success',\r\n                message: '支付成功!',\r\n              });\r\n              location.reload();\r\n            }else{\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        }).catch(fail=>{\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消'\r\n          });          \r\n        });\r\n      },\r\n      //确认收货\r\n      handleComfirm(index, row){\r\n        this.$confirm('是否确认收货?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(res=>{\r\n          this.$axios.put('/order/updatestate',{\r\n            oid:row.oid,\r\n            state:'已签收'\r\n          }).then(res=>{\r\n            if(res.data.code==200){\r\n              this.$message({\r\n                type: 'success',\r\n                message: '签收成功!',\r\n              });\r\n              location.reload();\r\n            }else{\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        }).catch(fail=>{\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消'\r\n          });          \r\n        });\r\n      },\r\n      //评论\r\n      handleComment(index,row){\r\n        this.$prompt('请输入商品评论', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          inputValidator: value => {return value.length>0},\r\n          inputErrorMessage: '请输入至少一个字符'\r\n        }).then(({ value }) => {\r\n          console.log(value)\r\n          this.$axios.put('/order/updatecomment',{\r\n            oid:row.oid,\r\n            comment:value\r\n          }).then(res=>{\r\n            if(res.data.code==200){\r\n              this.$message({\r\n                type: 'success',\r\n                message: '评论成功!',\r\n              });\r\n              location.reload();\r\n            }else{\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        }).catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '取消输入'\r\n          });       \r\n        });\r\n      },\r\n      //详情\r\n      handleDetail(index,row){\r\n        this.$router.push({\r\n          name:'ordersdetails',\r\n          query:{\r\n            data:JSON.stringify(row)\r\n          }\r\n        })\r\n      },\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.table{\r\n  margin:20px;\r\n}\r\n\r\n.btn{\r\n  margin: 0px;\r\n  margin-bottom: 20px;\r\n  margin-right: 20px;\r\n  float: right;\r\n}\r\n\r\n</style>"],"mappings":";AAmGA;EACAA,KAAA;IACA;MACAC,SAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,QAAA;EACA;EACAC,OAAA;IACA;IACAD,SAAA;MACA,KAAAE,MAAA,CAAAC,GAAA,2BAAAC,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAC,EAAA,EACAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAZ,IAAA,CAAAa,IAAA;UACA,KAAAZ,SAAA,GAAAW,GAAA,CAAAZ,IAAA,CAAAc,MAAA;QACA;UACA,KAAAC,MAAA,CAAAH,GAAA,CAAAZ,IAAA,CAAAgB,OAAA;YACAC,iBAAA;UACA;QACA;MACA;IACA;IACA;IACAC,UAAAC,KAAA,EAAAC,GAAA;MACA,KAAAC,QAAA;QACAJ,iBAAA;QACAK,gBAAA;QACAC,IAAA;MACA,GAAAZ,IAAA,CAAAC,GAAA;QACA,KAAAP,MAAA,CAAAmB,GAAA;UACAC,GAAA,EAAAL,GAAA,CAAAK,GAAA;UACAjB,KAAA;QACA,GAAAG,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAZ,IAAA,CAAAa,IAAA;YACA,KAAAa,QAAA;cACAH,IAAA;cACAP,OAAA;YACA;YACAW,QAAA,CAAAC,MAAA;UACA;YACA,KAAAF,QAAA;cACAH,IAAA;cACAP,OAAA,EAAAJ,GAAA,CAAAZ,IAAA,CAAAgB;YACA;UACA;QACA;MACA,GAAAa,KAAA,CAAAC,IAAA;QACA,KAAAJ,QAAA;UACAH,IAAA;UACAP,OAAA;QACA;MACA;IACA;IACA;IACAe,cAAAZ,KAAA,EAAAC,GAAA;MACA,KAAAC,QAAA;QACAJ,iBAAA;QACAK,gBAAA;QACAC,IAAA;MACA,GAAAZ,IAAA,CAAAC,GAAA;QACA,KAAAP,MAAA,CAAAmB,GAAA;UACAC,GAAA,EAAAL,GAAA,CAAAK,GAAA;UACAjB,KAAA;QACA,GAAAG,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAZ,IAAA,CAAAa,IAAA;YACA,KAAAa,QAAA;cACAH,IAAA;cACAP,OAAA;YACA;YACAW,QAAA,CAAAC,MAAA;UACA;YACA,KAAAF,QAAA;cACAH,IAAA;cACAP,OAAA,EAAAJ,GAAA,CAAAZ,IAAA,CAAAgB;YACA;UACA;QACA;MACA,GAAAa,KAAA,CAAAC,IAAA;QACA,KAAAJ,QAAA;UACAH,IAAA;UAC