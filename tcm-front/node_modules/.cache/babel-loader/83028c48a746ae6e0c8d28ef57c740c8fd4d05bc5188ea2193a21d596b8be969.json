{"ast":null,"code":"export default {\n  data() {\n    //验证手机号\n    var checkMobile = (rule, value, cb) => {\n      const regMobile = /^(0|86|17951)?(13[0-9]|15[0123456789]|17[678]|18[0-9]|14[57])[0-9]{8}$/;\n      if (regMobile.test(value)) {\n        //合法\n        return cb();\n      }\n      //不合法\n      cb(new Error('请输入合法的手机号！'));\n    };\n    return {\n      ruleForm: {},\n      rules: {\n        address: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        phone: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }, {\n          validator: checkMobile,\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.ruleForm = JSON.parse(this.$route.query.data);\n    this.ruleForm.address = this.$store.state.user.address;\n    this.ruleForm.phone = this.$store.state.user.phone;\n    this.ruleForm = JSON.parse(JSON.stringify(this.ruleForm));\n  },\n  methods: {\n    goBack() {\n      this.$router.back();\n    },\n    //提交\n    submitForm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          this.$axios.post('/order/insert', {\n            cid: this.ruleForm.id,\n            uid: this.$store.state.user.id,\n            number: this.ruleForm.number,\n            address: this.ruleForm.address,\n            phone: this.ruleForm.phone,\n            createtime: new Date(),\n            state: '未支付'\n          }).then(res => {\n            if (res.data.code == 200) {\n              this.$message({\n                type: 'success',\n                message: \"下单成功！\"\n              });\n            } else {\n              this.$message({\n                type: 'error',\n                message: res.data.message\n              });\n            }\n          });\n        } else {\n          this.$message({\n            type: 'error',\n            message: \"请检查输入信息！\"\n          });\n        }\n      });\n    },\n    //重置\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n    }\n  }\n};","map":{"version":3,"names":["data","checkMobile","rule","value","cb","regMobile","test","Error","ruleForm","rules","address","required","message","trigger","phone","validator","created","JSON","parse","$route","query","$store","state","user","stringify","methods","goBack","$router","back","submitForm","formName","$refs","validate","valid","$axios","post","cid","id","uid","number","createtime","Date","then","res","code","$message","type","resetForm","resetFields"],"sources":["src/views/Consumer/Cart/Order/index.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-page-header @back=\"goBack\" content=\"提交订单\" style=\"margin:20px;\"></el-page-header>\r\n\r\n    <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\" class=\"demo-ruleForm\">\r\n\r\n      <el-form-item prop=\"picturepath\">\r\n        <img v-if=\"ruleForm.picturepath\" :src=\"`http://localhost:8081/`+ruleForm.picturepath\" class=\"avatar\">\r\n        <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"商品\">\r\n        <template>{{ruleForm.name}}</template>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"品牌\">\r\n        <template>{{ruleForm.brand}}</template>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"数量\">\r\n        <template>{{ruleForm.number}}</template>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"总价\">\r\n        <template>{{ruleForm.number*ruleForm.price*ruleForm.discount}}</template>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"购买人\">\r\n        <template>{{this.$store.state.user.name}}</template>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"收货地址\" prop=\"address\">\r\n        <el-input v-model=\"ruleForm.address\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"联系方式\" prop=\"phone\">\r\n        <el-input v-model=\"ruleForm.phone\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">提交</el-button>\r\n        <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\r\n      </el-form-item>\r\n\r\n    </el-form>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n\r\n    //验证手机号\r\n    var checkMobile = (rule,value,cb) =>{\r\n      const regMobile = /^(0|86|17951)?(13[0-9]|15[0123456789]|17[678]|18[0-9]|14[57])[0-9]{8}$/;\r\n      if(regMobile.test(value)){\r\n        //合法\r\n        return cb();\r\n      }\r\n      //不合法\r\n      cb(new Error('请输入合法的手机号！'));\r\n    }\r\n\r\n    return {\r\n      ruleForm: {\r\n\r\n      },\r\n      rules: {\r\n        address: [{required: true, message: '', trigger: 'blur'}],\r\n        phone: [{required: true, message: '', trigger: 'blur'},{validator: checkMobile, trigger: 'blur'}],\r\n      },\r\n    };\r\n  },\r\n  created(){\r\n    this.ruleForm=JSON.parse(this.$route.query.data)\r\n    this.ruleForm.address=this.$store.state.user.address\r\n    this.ruleForm.phone=this.$store.state.user.phone\r\n    this.ruleForm=JSON.parse(JSON.stringify(this.ruleForm))\r\n  },\r\n  methods: {\r\n    goBack(){\r\n      this.$router.back()\r\n    },\r\n    //提交\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.$axios.post('/order/insert',{\r\n            cid:this.ruleForm.id,\r\n            uid:this.$store.state.user.id,\r\n            number:this.ruleForm.number,\r\n            address:this.ruleForm.address,\r\n            phone:this.ruleForm.phone,\r\n            createtime:new Date(),\r\n            state:'未支付'\r\n          }).then(res=>{\r\n            if(res.data.code==200){\r\n              this.$message({\r\n                type: 'success',\r\n                message: \"下单成功！\",\r\n              });\r\n            }else{\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: 'error',\r\n            message: \"请检查输入信息！\",\r\n          });\r\n        }\r\n      });\r\n    },\r\n    //重置\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.el-form{\r\n  margin: auto;\r\n  margin-top:20px;\r\n  width:50%;\r\n}\r\n</style>"],"mappings":"AAkDA;EACAA,KAAA;IAEA;IACA,IAAAC,WAAA,GAAAA,CAAAC,IAAA,EAAAC,KAAA,EAAAC,EAAA;MACA,MAAAC,SAAA;MACA,IAAAA,SAAA,CAAAC,IAAA,CAAAH,KAAA;QACA;QACA,OAAAC,EAAA;MACA;MACA;MACAA,EAAA,KAAAG,KAAA;IACA;IAEA;MACAC,QAAA,GAEA;MACAC,KAAA;QACAC,OAAA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAC,KAAA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;UAAAE,SAAA,EAAAd,WAAA;UAAAY,OAAA;QAAA;MACA;IACA;EACA;EACAG,QAAA;IACA,KAAAR,QAAA,GAAAS,IAAA,CAAAC,KAAA,MAAAC,MAAA,CAAAC,KAAA,CAAApB,IAAA;IACA,KAAAQ,QAAA,CAAAE,OAAA,QAAAW,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAb,OAAA;IACA,KAAAF,QAAA,CAAAM,KAAA,QAAAO,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAT,KAAA;IACA,KAAAN,QAAA,GAAAS,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAO,SAAA,MAAAhB,QAAA;EACA;EACAiB,OAAA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACA;IACAC,WAAAC,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAC,MAAA,CAAAC,IAAA;YACAC,GAAA,OAAA5B,QAAA,CAAA6B,EAAA;YACAC,GAAA,OAAAjB,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAc,EAAA;YACAE,MAAA,OAAA/B,QAAA,CAAA+B,MAAA;YACA7B,OAAA,OAAAF,QAAA,CAAAE,OAAA;YACAI,KAAA,OAAAN,QAAA,CAAAM,KAAA;YACA0B,UAAA,MAAAC,IAAA;YACAnB,KAAA;UACA,GAAAoB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAA3C,IAAA,CAAA4C,IAAA;cACA,KAAAC,QAAA;gBACAC,IAAA;gBACAlC,OAAA;cACA;YACA;cACA,KAAAiC,QAAA;gBACAC,IAAA;gBACAlC,OAAA,EAAA+B,GAAA,CAAA3C,IAAA,CAAAY;cACA;YACA;UACA;QACA;UACA,KAAAiC,QAAA;YACAC,IAAA;YACAlC,OAAA;UACA;QACA;MACA;IACA;IACA;IACAmC,UAAAjB,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAkB,WAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module"