{"ast":null,"code":"export default {\n  data() {\n    //验证手机号\n    var checkMobile = (rule, value, cb) => {\n      const regMobile = /^(0|86|17951)?(13[0-9]|15[0123456789]|17[678]|18[0-9]|14[57])[0-9]{8}$/;\n      if (regMobile.test(value)) {\n        //合法\n        return cb();\n      }\n      //不合法\n      cb(new Error('请输入合法的手机号！'));\n    };\n    return {\n      ruleForm: {\n        uid: '',\n        //address:'',\n        phone: ''\n      },\n      rules: {\n        address: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        phone: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }, {\n          validator: checkMobile,\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.ruleForm = JSON.parse(this.$route.query.data);\n    this.ruleForm.address = this.$store.state.user.address;\n  },\n  methods: {\n    goBack() {\n      this.$router.back();\n    },\n    //提交\n    submitForm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          this.$axios.put('/commodity/update', {}).then(res => {\n            if (res.data.code == 200) {\n              this.$message({\n                type: 'success',\n                message: \"下单成功！\"\n              });\n            } else {\n              this.$message({\n                type: 'error',\n                message: res.data.message\n              });\n            }\n          });\n        } else {\n          this.$message({\n            type: 'error',\n            message: \"请检查输入信息！\"\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","checkMobile","rule","value","cb","regMobile","test","Error","ruleForm","uid","phone","rules","address","required","message","trigger","validator","created","JSON","parse","$route","query","$store","state","user","methods","goBack","$router","back","submitForm","formName","$refs","validate","valid","$axios","put","then","res","code","$message","type"],"sources":["src/views/Consumer/Cart/Order/index.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-page-header @back=\"goBack\" content=\"提交订单\" style=\"margin:20px;\"></el-page-header>\r\n\r\n    <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\" class=\"demo-ruleForm\">\r\n\r\n      <el-form-item prop=\"picturepath\">\r\n        <el-upload\r\n          disabled\r\n          class=\"avatar-uploader\"\r\n          action=\"http://localhost:8081/upload/commodity\"\r\n          :show-file-list=\"false\"\r\n          :on-success=\"handleAvatarSuccess\"\r\n          :before-upload=\"beforeAvatarUpload\"\r\n          style=\"margin:50px;\">\r\n          <img v-if=\"ruleForm.picturepath\" :src=\"`http://localhost:8081/`+ruleForm.picturepath\" class=\"avatar\">\r\n          <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n        </el-upload>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"商品\">\r\n        <template>{{ruleForm.name}}</template>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"品牌\">\r\n        <template>{{ruleForm.brand}}</template>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"数量\">\r\n        <template>{{ruleForm.number}}</template>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"总价\">\r\n        <template>{{ruleForm.number*ruleForm.price*ruleForm.discount}}</template>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"购买人\">\r\n        <template>{{this.$store.state.user.name}}</template>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"收货地址\">\r\n        <el-input v-model=\"ruleForm.address\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"联系方式\">\r\n        <el-input v-model=\"ruleForm.phone\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">提交</el-button>\r\n        <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\r\n      </el-form-item>\r\n\r\n    </el-form>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n\r\n    //验证手机号\r\n    var checkMobile = (rule,value,cb) =>{\r\n      const regMobile = /^(0|86|17951)?(13[0-9]|15[0123456789]|17[678]|18[0-9]|14[57])[0-9]{8}$/;\r\n      if(regMobile.test(value)){\r\n        //合法\r\n        return cb();\r\n      }\r\n      //不合法\r\n      cb(new Error('请输入合法的手机号！'));\r\n    }\r\n\r\n    return {\r\n      ruleForm: {\r\n        uid:'',\r\n        //address:'',\r\n        phone:'',\r\n      },\r\n      rules: {\r\n        address: [{required: true, message: '', trigger: 'blur'}],\r\n        phone: [{required: true, message: '', trigger: 'blur'},{validator: checkMobile, trigger: 'blur'}],\r\n      },\r\n    };\r\n  },\r\n  created(){\r\n    this.ruleForm=JSON.parse(this.$route.query.data)\r\n    this.ruleForm.address=this.$store.state.user.address\r\n  },\r\n  methods: {\r\n    goBack(){\r\n      this.$router.back()\r\n    },\r\n    //提交\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.$axios.put('/commodity/update',{\r\n            \r\n          }).then(res=>{\r\n            if(res.data.code==200){\r\n              this.$message({\r\n                type: 'success',\r\n                message: \"下单成功！\",\r\n              });\r\n            }else{\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: 'error',\r\n            message: \"请检查输入信息！\",\r\n          });\r\n        }\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.el-form{\r\n  margin: auto;\r\n  margin-top:20px;\r\n  width:50%;\r\n}\r\n</style>"],"mappings":"AA2DA;EACAA,KAAA;IAEA;IACA,IAAAC,WAAA,GAAAA,CAAAC,IAAA,EA