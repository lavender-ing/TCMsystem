{"ast":null,"code":"export default {\n  data() {\n    //验证邮箱\n    var checkEmail = (rule, value, cb) => {\n      const regEmail = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(\\.[a-zA-Z0-9_-])+/;\n      if (regEmail.test(value)) {\n        //合法\n        return cb();\n      }\n      //不合法\n      cb(new Error('请输入合法的邮箱！'));\n    };\n\n    //验证手机号\n    var checkMobile = (rule, value, cb) => {\n      const regMobile = /^(0|86|17951)?(13[0-9]|15[0123456789]|17[678]|18[0-9]|14[57])[0-9]{8}$/;\n      if (regMobile.test(value)) {\n        //合法\n        return cb();\n      }\n      //不合法\n      cb(new Error('请输入合法的手机号！'));\n    };\n    return {\n      ruleForm: {\n        username: '',\n        password: '',\n        name: '',\n        phone: '',\n        mail: '',\n        address: '',\n        sex: '男',\n        type: '买家账号'\n      },\n      rules: {\n        username: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        name: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        phone: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }, {\n          validator: checkMobile,\n          trigger: 'blur'\n        }],\n        mail: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }, {\n          validator: checkEmail,\n          trigger: 'blur'\n        }],\n        address: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        sex: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        type: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {},\n  methods: {\n    submitForm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          this.$axios.post('/user/insert', {\n            username: this.ruleForm.username,\n            password: this.ruleForm.password,\n            name: this.ruleForm.name,\n            phone: this.ruleForm.phone,\n            mail: this.ruleForm.mail,\n            address: this.ruleForm.address,\n            sex: this.ruleForm.sex,\n            state: '正常',\n            registertime: new Date(),\n            lastlogin: new Date(),\n            type: this.ruleForm.type\n          }).then(res => {\n            if (res.data.code == 200) {\n              this.$message({\n                type: 'success',\n                message: \"注册成功！\"\n              });\n              this.$axios.get('/login/checklogin', {\n                params: {\n                  username: this.ruleForm.username,\n                  password: this.ruleForm.password\n                }\n              }).then(res => {\n                /*登陆成功*/\n                if (res.data.code == 200) {\n                  this.$store.commit('login', {\n                    user: res.data.result,\n                    isLogin: true\n                  });\n                  this.$router.replace('/');\n                } else {\n                  /*登陆失败*/\n                  this.$alert(res.data.message, '提示', {\n                    confirmButtonText: '确定'\n                  });\n                }\n              });\n            } else {\n              this.$message({\n                type: 'error',\n                message: res.data.message\n              });\n            }\n          });\n        } else {\n          this.$message({\n            type: 'error',\n            message: \"请检查输入信息！\"\n          });\n        }\n      });\n    },\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n    }\n  }\n};","map":{"version":3,"names":["data","checkEmail","rule","value","cb","regEmail","test","Error","checkMobile","regMobile","ruleForm","username","password","name","phone","mail","address","sex","type","rules","required","message","trigger","validator","created","methods","submitForm","formName","$refs","validate","valid","$axios","post","state","registertime","Date","lastlogin","then","res","code","$message","get","params","$store","commit","user","result","isLogin","$router","replace","$alert","confirmButtonText","resetForm","resetFields"],"sources":["src/views/Register/index.vue"],"sourcesContent":["<template>\r\n  <div style=\"margin:30px\">\r\n    <h1>用户注册</h1>\r\n    <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\" class=\"demo-ruleForm\">\r\n\r\n      <el-form-item label=\"用户名\" prop=\"username\">\r\n        <el-input v-model=\"ruleForm.username\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"密码\" prop=\"password\">\r\n        <el-input v-model=\"ruleForm.password\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"真实姓名\" prop=\"name\">\r\n        <el-input v-model=\"ruleForm.name\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"联系电话\" prop=\"phone\">\r\n        <el-input v-model=\"ruleForm.phone\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"邮箱\" prop=\"mail\">\r\n        <el-input v-model=\"ruleForm.mail\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"住址\" prop=\"address\">\r\n        <el-input v-model=\"ruleForm.address\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"用户性别\" prop=\"sex\">\r\n        <el-radio-group v-model=\"ruleForm.sex\">\r\n          <el-radio label=\"男\" ></el-radio>\r\n          <el-radio label=\"女\"></el-radio>\r\n        </el-radio-group>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"用户类型\" prop=\"type\">\r\n        <el-radio-group v-model=\"ruleForm.type\">\r\n          <el-radio label=\"运营商账号\" ></el-radio>\r\n          <el-radio label=\"买家账号\"></el-radio>\r\n        </el-radio-group>\r\n      </el-form-item>\r\n      \r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">提交</el-button>\r\n        <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\r\n      </el-form-item>\r\n\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n\r\n      //验证邮箱\r\n      var checkEmail = (rule,value,cb) => {\r\n        const regEmail = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(\\.[a-zA-Z0-9_-])+/;\r\n        if(regEmail.test(value)){\r\n          //合法\r\n          return cb();\r\n        }\r\n        //不合法\r\n        cb(new Error('请输入合法的邮箱！'));\r\n      }\r\n\r\n      //验证手机号\r\n      var checkMobile = (rule,value,cb) =>{\r\n        const regMobile = /^(0|86|17951)?(13[0-9]|15[0123456789]|17[678]|18[0-9]|14[57])[0-9]{8}$/;\r\n        if(regMobile.test(value)){\r\n          //合法\r\n          return cb();\r\n        }\r\n        //不合法\r\n        cb(new Error('请输入合法的手机号！'));\r\n      }\r\n\r\n    return {\r\n      ruleForm: {\r\n        username:'',\r\n        password:'',\r\n        name:'',\r\n        phone:'',\r\n        mail:'',\r\n        address:'',\r\n        sex:'男',\r\n        type:'买家账号',\r\n      },\r\n      rules: {\r\n        username: [{required: true, message: '', trigger: 'blur'}],\r\n        password: [{required: true, message: '', trigger: 'blur'}],\r\n        name: [{required: true, message: '', trigger: 'blur'}],\r\n        phone: [{required: true, message: '', trigger: 'blur'},{validator: checkMobile, trigger: 'blur'}],\r\n        mail: [{required: true, message: '', trigger: 'blur'},{validator: checkEmail, trigger: 'blur'}],\r\n        address: [{required: true, message: '', trigger: 'blur'}],\r\n        sex: [{required: true, message: '', trigger: 'blur'}],\r\n        type: [{required: true, message: '', trigger: 'blur'}],\r\n      }\r\n    };\r\n  },\r\n  created(){\r\n\r\n  },\r\n  methods: {\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.$axios.post('/user/insert',{\r\n            username:this.ruleForm.username,\r\n            password:this.ruleForm.password,\r\n            name:this.ruleForm.name,\r\n            phone:this.ruleForm.phone,\r\n            mail:this.ruleForm.mail,\r\n            address:this.ruleForm.address,\r\n            sex:this.ruleForm.sex,\r\n            state:'正常',\r\n            registertime:new Date(),\r\n            lastlogin:new Date(),\r\n            type:this.ruleForm.type\r\n          }).then(res=>{\r\n            if(res.data.code==200){\r\n              this.$message({\r\n                type: 'success',\r\n                message: \"注册成功！\",\r\n              });\r\n              this.$axios.get('/login/checklogin',{\r\n                params:{\r\n                  username:this.ruleForm.username,\r\n                  password:this.ruleForm.password\r\n                }\r\n              }).then(res=>{\r\n                /*登陆成功*/\r\n                if(res.data.code==200){\r\n                  this.$store.commit('login',{\r\n                    user:res.data.result,\r\n                    isLogin:true\r\n                  })\r\n                  this.$router.replace('/')\r\n                }else{\r\n                  /*登陆失败*/\r\n                  this.$alert(res.data.message, '提示', {\r\n                      confirmButtonText: '确定'\r\n                  })\r\n                }\r\n              })\r\n            }else{\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: 'error',\r\n            message: \"请检查输入信息！\",\r\n          });\r\n        }\r\n      });\r\n    },\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.el-form{\r\n  margin: auto;\r\n  margin-top:20px;\r\n  width:50%;\r\n}\r\n\r\n</style>"],"mappings":"AAqDA;EACAA,KAAA;IAEA;IACA,IAAAC,UAAA,GAAAA,CAAAC,IAAA,EAAAC,KAAA,EAAAC,EAAA;MACA,MAAAC,QAAA;MACA,IAAAA,QAAA,CAAAC,IAAA,CAAAH,KAAA;QACA;QACA,OAAAC,EAAA;MACA;MACA;MACAA,EAAA,KAAAG,KAAA;IACA;;IAEA;IACA,IAAAC,WAAA,GAAAA,CAAAN,IAAA,EAAAC,KAAA,EAAAC,EAAA;MACA,MAAAK,SAAA;MACA,IAAAA,SAAA,CAAAH,IAAA,CAAAH,KAAA;QACA;QACA,OAAAC,EAAA;MACA;MACA;MACAA,EAAA,KAAAG,KAAA;IACA;IAEA;MACAG,QAAA;QACAC,QAAA;QACAC,QAAA;QACAC,IAAA;QACAC,KAAA;QACAC,IAAA;QACAC,OAAA;QACAC,GAAA;QACAC,IAAA;MACA;MACAC,KAAA;QACAR,QAAA;UAAAS,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAV,QAAA;UAAAQ,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAT,IAAA;UAAAO,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAR,KAAA;UAAAM,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;UAAAC,SAAA,EAAAf,WAAA;UAAAc,OAAA;QAAA;QACAP,IAAA;UAAAK,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;UAAAC,SAAA,EAAAtB,UAAA;UAAAqB,OAAA;QAAA;QACAN,OAAA;UAAAI,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAL,GAAA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAJ,IAAA;UAAAE,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;IACA;EACA;EACAE,QAAA,GAEA;EACAC,OAAA;IACAC,WAAAC,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAC,MAAA,CAAAC,IAAA;YACArB,QAAA,OAAAD,QAAA,CAAAC,QAAA;YACAC,QAAA,OAAAF,QAAA,CAAAE,QAAA;YACAC,IAAA,OAAAH,QAAA,CAAAG,IAAA;YACAC,KAAA,OAAAJ,QAAA,CAAAI,KAAA;YACAC,IAAA,OAAAL,QAAA,CAAAK,IAAA;YACAC,OAAA,OAAAN,QAAA,CAAAM,OAAA;YACAC,GAAA,OAAAP,QAAA,CAAAO,GAAA;YACAgB,KAAA;YACAC,YAAA,MAAAC,IAAA;YACAC,SAAA,MAAAD,IAAA;YACAjB,IAAA,OAAAR,QAAA,CAAAQ;UACA,GAAAmB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAtC,IAAA,CAAAuC,IAAA;cACA,KAAAC,QAAA;gBACAtB,IAAA;gBACAG,OAAA;cACA;cACA,KAAAU,MAAA,CAAAU,GAAA;gBACAC,MAAA;kBACA/B,QAAA,OAAAD,QAAA,CAAAC,QAAA;kBACAC,QAAA,OAAAF,QAAA,CAAAE;gBACA;cACA,GAAAyB,IAAA,CAAAC,GAAA;gBACA;gBACA,IAAAA,GAAA,CAAAtC,IAAA,CAAAuC,IAAA;kBACA,KAAAI,MAAA,CAAAC,MAAA;oBACAC,IAAA,EAAAP,GAAA,CAAAtC,IAAA,CAAA8C,MAAA;oBACAC,OAAA;kBACA;kBACA,KAAAC,OAAA,CAAAC,OAAA;gBACA;kBACA;kBACA,KAAAC,MAAA,CAAAZ,GAAA,CAAAtC,IAAA,CAAAqB,OAAA;oBACA8B,iBAAA;kBACA;gBACA;cACA;YACA