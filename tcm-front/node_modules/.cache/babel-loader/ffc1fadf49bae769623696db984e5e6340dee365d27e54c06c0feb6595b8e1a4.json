{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      tableData: [],\n      srcList: ['']\n    };\n  },\n  created() {\n    this.loadData();\n  },\n  methods: {\n    //加载信息\n    loadData() {\n      this.$axios.get('/cart/getall/' + this.$store.state.user.id).then(res => {\n        if (res.data.code == 200) {\n          this.tableData = res.data.result;\n        } else {\n          this.$alert(res.data.message, '提示', {\n            confirmButtonText: '确定'\n          });\n        }\n      });\n    },\n    //新增\n    add() {\n      this.$router.push({\n        name: 'commodityadd'\n      });\n    },\n    //修改\n    handleUpdate(index, row) {\n      this.$router.push({\n        name: 'commodityupdate',\n        query: {\n          data: JSON.stringify(row)\n        }\n      });\n    },\n    //删除（下架）\n    handleDelete(index, row) {\n      this.$confirm('是否下架该商品?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(res => {\n        this.$axios.delete('/commodity/delete/' + row.id).then(res => {\n          if (res.data.code == 200) {\n            //删除成功\n            this.$message({\n              type: 'success',\n              message: '下架成功!'\n            });\n            location.reload();\n          } else {\n            //删除失败\n            this.$message({\n              type: 'error',\n              message: res.data.message\n            });\n          }\n        });\n      }).catch(fail => {\n        this.$message({\n          type: 'info',\n          message: '已取消'\n        });\n      });\n    },\n    //上架\n    handleSell(index, row) {\n      this.$confirm('是否上架该商品?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(res => {\n        this.$axios.put('/commodity/sell/' + row.id).then(res => {\n          if (res.data.code == 200) {\n            //删除成功\n            this.$message({\n              type: 'success',\n              message: '上架成功!'\n            });\n            location.reload();\n          } else {\n            //删除失败\n            this.$message({\n              type: 'error',\n              message: res.data.message\n            });\n          }\n        });\n      }).catch(fail => {\n        this.$message({\n          type: 'info',\n          message: '已取消'\n        });\n      });\n    },\n    //加入购物车\n    handleAdd(index, row) {\n      this.$confirm('是否将该商品加入购物车?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(res => {\n        this.$axios.post('/cart/insert', {\n          cid: row.id,\n          uid: this.$store.state.user.id\n        }).then(res => {\n          if (res.data.code == 200) {\n            this.$message({\n              type: 'success',\n              message: \"添加成功！\"\n            });\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.data.message\n            });\n          }\n        });\n      }).catch(fail => {\n        this.$message({\n          type: 'info',\n          message: '已取消'\n        });\n      });\n    },\n    //详情\n    handleDetails(index, row) {\n      this.$router.push({\n        name: 'commoditydetails',\n        query: {\n          data: JSON.stringify(row)\n        }\n      });\n    },\n    //大图\n    bigImg(url) {\n      this.srcList[0] = url;\n    }\n  }\n};","map":{"version":3,"names":["data","tableData","srcList","created","loadData","methods","$axios","get","$store","state","user","id","then","res","code","result","$alert","message","confirmButtonText","add","$router","push","name","handleUpdate","index","row","query","JSON","stringify","handleDelete","$confirm","cancelButtonText","type","delete","$message","location","reload","catch","fail","handleSell","put","handleAdd","post","cid","uid","handleDetails","bigImg","url"],"sources":["src/views/Consumer/Cart/List/index.vue"],"sourcesContent":["<template>\r\n  <div>\r\n\r\n    <div class=\"btn\" v-if=\"this.$store.state.user.type=='运营商'\">\r\n      <el-button type=\"primary\" size=\"small\" @click=\"add\">新增商品</el-button>\r\n    </div>\r\n\r\n    <div class=\"table\">\r\n      <el-table\r\n        :data=\"tableData\"\r\n        :border=true\r\n        style=\"width: 100%\"\r\n        :stripe=true\r\n        :header-cell-style=\"{'text-align':'center'}\"\r\n        :cell-style=\"{'text-align':'center'}\">\r\n\r\n        <el-table-column\r\n          prop=\"name\"\r\n          label=\"商品\"\r\n          :show-overflow-tooltip=true>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"brand\"\r\n          label=\"品牌\"\r\n          :show-overflow-tooltip=true>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"price\"\r\n          label=\"原价\"\r\n          :show-overflow-tooltip=true>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"discount\"\r\n          label=\"折扣价\"\r\n          :show-overflow-tooltip=true>\r\n          <template slot-scope=\"scope\">{{scope.row.discount*scope.row.price}}</template>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          label=\"图片\"\r\n          prop=\"picturepath\"\r\n          :show-overflow-tooltip=true>\r\n          <template slot-scope=\"scope\">\r\n            <el-image\r\n              v-if=\"scope.row.picturepath\"\r\n              @click=\"\r\n                bigImg(`http://localhost:8081/`+scope.row.picturepath)\"\r\n              style=\"width: 50px; height: 50px; margin-bottom:-4px\"\r\n              :src=\"`http://localhost:8081/`+scope.row.picturepath\"\r\n              :preview-src-list=\"srcList\">\r\n            </el-image>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"number\"\r\n          label=\"数量\"\r\n          :show-overflow-tooltip=true>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          label=\"总价\"\r\n          :show-overflow-tooltip=true>\r\n          <template slot-scope=\"scope\">\r\n            {{scope.row.price*scope.row.discount*scope.row.number}}\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"mini\"\r\n              @click=\"handleAdd(scope.$index, scope.row)\">加入购物车</el-button>\r\n\r\n              <el-button\r\n              size=\"mini\"\r\n              type=\"info\"\r\n              @click=\"handleDetails(scope.$index, scope.row)\">详情</el-button>\r\n          </template>\r\n        </el-table-column>\r\n\r\n      </el-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      srcList:['']\r\n    }\r\n  },\r\n  created(){\r\n    this.loadData()\r\n  },\r\n  methods: {\r\n      //加载信息\r\n      loadData(){\r\n        this.$axios.get('/cart/getall/'+this.$store.state.user.id)\r\n        .then(res=>{\r\n          if(res.data.code==200){\r\n            this.tableData=res.data.result\r\n          }else{\r\n            this.$alert(res.data.message, '提示', {\r\n                confirmButtonText: '确定'\r\n            })\r\n          }\r\n        })\r\n      },\r\n      //新增\r\n      add(){\r\n        this.$router.push({\r\n          name:'commodityadd'\r\n        })\r\n      },\r\n      //修改\r\n      handleUpdate(index, row) {\r\n        this.$router.push({\r\n          name:'commodityupdate',\r\n          query:{\r\n            data:JSON.stringify(row)\r\n          }\r\n        })\r\n      },\r\n      //删除（下架）\r\n      handleDelete(index, row) {\r\n        this.$confirm('是否下架该商品?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(res=>{\r\n          this.$axios.delete('/commodity/delete/'+row.id)\r\n          .then(res=>{\r\n            if(res.data.code==200){\r\n              //删除成功\r\n              this.$message({\r\n                type: 'success',\r\n                message: '下架成功!',\r\n              });\r\n              location.reload();\r\n            }else{\r\n              //删除失败\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        }).catch(fail=>{\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消'\r\n          });          \r\n        });\r\n      },\r\n      //上架\r\n      handleSell(index, row) {\r\n        this.$confirm('是否上架该商品?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(res=>{\r\n          this.$axios.put('/commodity/sell/'+row.id)\r\n          .then(res=>{\r\n            if(res.data.code==200){\r\n              //删除成功\r\n              this.$message({\r\n                type: 'success',\r\n                message: '上架成功!',\r\n              });\r\n              location.reload();\r\n            }else{\r\n              //删除失败\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        }).catch(fail=>{\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消'\r\n          });          \r\n        });\r\n      },\r\n      //加入购物车\r\n      handleAdd(index, row) {\r\n        this.$confirm('是否将该商品加入购物车?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(res=>{\r\n          this.$axios.post('/cart/insert',{\r\n            cid:row.id,\r\n            uid:this.$store.state.user.id\r\n          }).then(res=>{\r\n            if(res.data.code==200){\r\n              this.$message({\r\n                type: 'success',\r\n                message: \"添加成功！\",\r\n              });\r\n            }else{\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        }).catch(fail=>{\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消'\r\n          });          \r\n        });\r\n      },\r\n      //详情\r\n      handleDetails(index, row) {\r\n        this.$router.push({\r\n          name:'commoditydetails',\r\n          query:{\r\n            data:JSON.stringify(row)\r\n          }\r\n        })\r\n      },\r\n      //大图\r\n      bigImg(url) {\r\n        this.srcList[0] = url;\r\n      },\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.table{\r\n  margin:20px;\r\n}\r\n\r\n.btn{\r\n  margin: 0px;\r\n  margin-bottom: 20px;\r\n  margin-right: 20px;\r\n  float: right;\r\n}\r\n\r\n</style>"],"mappings":";AA0FA;EACAA,KAAA;IACA;MACAC,SAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,QAAA;EACA;EACAC,OAAA;IACA;IACAD,SAAA;MACA,KAAAE,MAAA,CAAAC,GAAA,wBAAAC,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAC,EAAA,EACAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAb,IAAA,CAAAc,IAAA;UACA,KAAAb,SAAA,GAAAY,GAAA,CAAAb,IAAA,CAAAe,MAAA;QACA;UACA,KAAAC,MAAA,CAAAH,GAAA,CAAAb,IAAA,CAAAiB,OAAA;YACAC,iBAAA;UACA;QACA;MACA;IACA;IACA;IACAC,IAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QACAC,IAAA;MACA;IACA;IACA;IACAC,aAAAC,KAAA,EAAAC,GAAA;MACA,KAAAL,OAAA,CAAAC,IAAA;QACAC,IAAA;QACAI,KAAA;UACA1B,IAAA,EAAA2B,IAAA,CAAAC,SAAA,CAAAH,GAAA;QACA;MACA;IACA;IACA;IACAI,aAAAL,KAAA,EAAAC,GAAA;MACA,KAAAK,QAAA;QACAZ,iBAAA;QACAa,gBAAA;QACAC,IAAA;MACA,GAAApB,IAAA,CAAAC,GAAA;QACA,KAAAP,MAAA,CAAA2B,MAAA,wBAAAR,GAAA,CAAAd,EAAA,EACAC,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAb,IAAA,CAAAc,IAAA;YACA;YACA,KAAAoB,QAAA;cACAF,IAAA;cACAf,OAAA;YACA;YACAkB,QAAA,CAAAC,MAAA;UACA;YACA;YACA,KAAAF,QAAA;cACAF,IAAA;cACAf,OAAA,EAAAJ,GAAA,CAAAb,IAAA,CAAAiB;YACA;UACA;QACA;MACA,GAAAoB,KAAA,CAAAC,IAAA;Q