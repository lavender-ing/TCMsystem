{"ast":null,"code":"export default {\n  data() {\n    //价格大于0\n    var checkPrice = (rule, value, cb) => {\n      if (value > 0) {\n        //合法\n        return cb();\n      }\n      //不合法\n      cb(new Error('价格应大于等于0！'));\n    };\n\n    //折扣\n    var checkDiscount = (rule, value, cb) => {\n      if (value >= 0 && value <= 1) {\n        //合法\n        return cb();\n      }\n      //不合法\n      cb(new Error('折扣应在0~1之间！'));\n    };\n    return {\n      ruleForm: {},\n      rules: {\n        name: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        brand: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        price: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }, {\n          validator: checkPrice,\n          trigger: 'blur'\n        }],\n        discount: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }, {\n          validator: checkDiscount,\n          trigger: 'blur'\n        }],\n        type: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        description: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        picturepath: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }]\n      },\n      types: []\n    };\n  },\n  created() {\n    this.loadData();\n  },\n  methods: {\n    goBack() {\n      this.$router.back();\n    },\n    //加载数据\n    loadData() {\n      this.$axios.get('/type/getall').then(res => {\n        if (res.data.code == 200) {\n          this.types = res.data.result;\n        } else {\n          this.$alert(res.data.message, '提示', {\n            confirmButtonText: '确定'\n          });\n        }\n      });\n    },\n    submitForm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          this.$axios.post('/commodity/insert', {\n            name: this.ruleForm.name,\n            brand: this.ruleForm.brand,\n            price: this.ruleForm.price,\n            discount: this.ruleForm.discount,\n            description: this.ruleForm.description,\n            picturepath: this.ruleForm.picturepath,\n            state: true,\n            type: this.ruleForm.type\n          }).then(res => {\n            if (res.data.code == 200) {\n              this.$message({\n                type: 'success',\n                message: \"添加成功！\"\n              });\n            } else {\n              this.$message({\n                type: 'error',\n                message: res.data.message\n              });\n            }\n          });\n        } else {\n          this.$message({\n            type: 'error',\n            message: \"请检查输入信息！\"\n          });\n        }\n      });\n    },\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n    },\n    //图片上传\n    handleAvatarSuccess(res, file) {\n      this.ruleForm.picturepath = res.result;\n      console.log(res);\n      console.log(this.ruleForm.picturepath);\n    },\n    beforeAvatarUpload(file) {\n      const isJPG = file.type === 'image/jpeg';\n      const isLt2M = file.size / 1024 / 1024 < 2;\n      if (!isJPG) {\n        this.$message.error('上传头像图片只能是 JPG 格式!');\n      }\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 2MB!');\n      }\n      return isJPG && isLt2M;\n    }\n  }\n};","map":{"version":3,"names":["data","checkPrice","rule","value","cb","Error","checkDiscount","ruleForm","rules","name","required","message","trigger","brand","price","validator","discount","type","description","picturepath","types","created","loadData","methods","goBack","$router","back","$axios","get","then","res","code","result","$alert","confirmButtonText","submitForm","formName","$refs","validate","valid","post","state","$message","resetForm","resetFields","handleAvatarSuccess","file","console","log","beforeAvatarUpload","isJPG","isLt2M","size","error"],"sources":["src/views/Operator/Commodity/Add/index.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-page-header @back=\"goBack\" content=\"新增商品\" style=\"margin:20px;\"></el-page-header>\r\n\r\n    <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\" class=\"demo-ruleForm\">\r\n\r\n      <el-form-item prop=\"picturepath\">\r\n        <el-upload\r\n          class=\"avatar-uploader\"\r\n          action=\"http://localhost:8081/upload/commodity\"\r\n          :show-file-list=\"false\"\r\n          :on-success=\"handleAvatarSuccess\"\r\n          :before-upload=\"beforeAvatarUpload\"\r\n          style=\"margin:50px;\">\r\n          <img v-if=\"ruleForm.picturepath\" :src=\"`http://localhost:8081/`+ruleForm.picturepath\" class=\"avatar\">\r\n          <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n        </el-upload>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"商品\" prop=\"name\">\r\n        <el-input v-model=\"ruleForm.name\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"品牌\" prop=\"brand\">\r\n        <el-input v-model=\"ruleForm.brand\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"原价\" prop=\"price\">\r\n        <el-input v-model=\"ruleForm.price\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"折扣\" prop=\"discount\">\r\n        <el-input v-model=\"ruleForm.discount\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"商品类型\" prop=\"type\">\r\n        <el-select v-model=\"ruleForm.type\" placeholder=\"请选择\">\r\n          <el-option\r\n            v-for=\"item in types\"\r\n            :key=\"item.id\"\r\n            :label=\"item.name\"\r\n            :value=\"item.name\">\r\n          </el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"描述\" prop=\"description\">\r\n        <el-input v-model=\"ruleForm.description\"></el-input>\r\n      </el-form-item>\r\n      \r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">提交</el-button>\r\n        <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\r\n      </el-form-item>\r\n\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n\r\n    //价格大于0\r\n    var checkPrice = (rule,value,cb) =>{\r\n      if(value>0){\r\n        //合法\r\n        return cb();\r\n      }\r\n      //不合法\r\n      cb(new Error('价格应大于等于0！'));\r\n    }\r\n\r\n    //折扣\r\n    var checkDiscount = (rule,value,cb) =>{\r\n      if(value>=0 && value<=1){\r\n        //合法\r\n        return cb();\r\n      }\r\n      //不合法\r\n      cb(new Error('折扣应在0~1之间！'));\r\n    }\r\n\r\n    return {\r\n      ruleForm: {\r\n        \r\n      },\r\n      rules: {\r\n        name: [{required: true, message: '', trigger: 'blur'}],\r\n        brand: [{required: true, message: '', trigger: 'blur'}],\r\n        price: [{required: true, message: '', trigger: 'blur'},{validator: checkPrice, trigger: 'blur'}],\r\n        discount: [{required: true, message: '', trigger: 'blur'},{validator: checkDiscount, trigger: 'blur'}],\r\n        type: [{required: true, message: '', trigger: 'blur'}],\r\n        description: [{required: true, message: '', trigger: 'blur'}],\r\n        picturepath: [{required: true, message: '', trigger: 'blur'}],\r\n      },\r\n      types:[]\r\n    };\r\n  },\r\n  created(){\r\n    this.loadData()\r\n  },\r\n  methods: {\r\n    goBack(){\r\n      this.$router.back()\r\n    },\r\n    //加载数据\r\n    loadData(){\r\n      this.$axios.get('/type/getall')\r\n        .then(res=>{\r\n          if(res.data.code==200){\r\n            this.types=res.data.result\r\n          }else{\r\n            this.$alert(res.data.message, '提示', {\r\n                confirmButtonText: '确定'\r\n            })\r\n          }\r\n        })\r\n    },\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.$axios.post('/commodity/insert',{\r\n            name:this.ruleForm.name,\r\n            brand:this.ruleForm.brand,\r\n            price:this.ruleForm.price,\r\n            discount:this.ruleForm.discount,\r\n            description:this.ruleForm.description,\r\n            picturepath:this.ruleForm.picturepath,\r\n            state:true,\r\n            type:this.ruleForm.type\r\n          }).then(res=>{\r\n            if(res.data.code==200){\r\n              this.$message({\r\n                type: 'success',\r\n                message: \"添加成功！\",\r\n              });\r\n            }else{\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: 'error',\r\n            message: \"请检查输入信息！\",\r\n          });\r\n        }\r\n      });\r\n    },\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n    //图片上传\r\n    handleAvatarSuccess(res, file) {\r\n      this.ruleForm.picturepath=res.result\r\n      console.log(res)\r\n      console.log(this.ruleForm.picturepath)\r\n    },\r\n\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === 'image/jpeg';\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n\r\n      if (!isJPG) {\r\n        this.$message.error('上传头像图片只能是 JPG 格式!');\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error('上传头像图片大小不能超过 2MB!');\r\n      }\r\n      return isJPG && isLt2M;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.el-form{\r\n  margin: auto;\r\n  margin-top:20px;\r\n  width:50%;\r\n}\r\n\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409EFF;\r\n}\r\n.