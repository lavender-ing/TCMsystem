{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {};\n  },\n  props: {\n    \"list\": {\n      type: Array,\n      default: () => {}\n    }\n  },\n  created() {},\n  methods: {\n    //详情\n    handleDetail(item) {\n      this.$router.push({\n        path: '/operator/commodity/details',\n        query: {\n          data: JSON.stringify(item)\n        }\n      });\n    },\n    //加入购物车\n    handleAdd(id) {\n      this.$confirm('是否将该商品加入购物车?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(res => {\n        this.$axios.post('/cart/insert', {\n          cid: id,\n          uid: this.$store.state.user.id\n        }).then(res => {\n          if (res.data.code == 200) {\n            this.$message({\n              type: 'success',\n              message: \"添加成功！\"\n            });\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.data.message\n            });\n          }\n        });\n      }).catch(fail => {\n        this.$message({\n          type: 'info',\n          message: '已取消'\n        });\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","props","type","Array","default","created","methods","handleDetail","item","$router","push","path","query","JSON","stringify","handleAdd","id","$confirm","confirmButtonText","cancelButtonText","then","res","$axios","post","cid","uid","$store","state","user","code","$message","message","catch","fail"],"sources":["src/components/CommodityCard/index.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-row class=\"row\" :gutter=\"10\">\r\n      <el-col :span=\"4\" v-for=\"(item,index) in list\" :key=\"index\" :offset=\"0\">\r\n        <el-card :body-style=\"{ padding: '0px' }\" class=\"card\">\r\n          <div style=\"height: 200px;\">\r\n            <el-image\r\n              class=\"image\"\r\n              :src=\"`http://localhost:8081/`+item.picturepath\">\r\n            </el-image>\r\n          </div>\r\n          <div style=\"height:50px;padding: 14px;\">\r\n            <span style=\"font-weight: bold;\">{{ item.name }}</span>\r\n            <span class=\"description\">售价：{{ item.price*item.discount }}</span>\r\n            <div class=\"bottom clearfix\">\r\n              <el-button type=\"text\" class=\"button\" @click=\"handleAdd(item.id)\">加入购物车</el-button>\r\n              <el-button type=\"text\" class=\"button\" @click=\"handleDetail(item)\">商品详情</el-button>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n\r\n    };\r\n  },\r\n  props:{\r\n    \"list\":{type:Array,default: () => {}}\r\n  },\r\n  created(){\r\n\r\n  },\r\n  methods:{\r\n    //详情\r\n    handleDetail(item) {\r\n      this.$router.push({\r\n        path:'/operator/commodity/details',\r\n        query:{\r\n          data:JSON.stringify(item)\r\n        }\r\n      })\r\n    },\r\n    //加入购物车\r\n    handleAdd(id) {\r\n      this.$confirm('是否将该商品加入购物车?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(res=>{\r\n        this.$axios.post('/cart/insert',{\r\n          cid:id,\r\n          uid:this.$store.state.user.id\r\n        }).then(res=>{\r\n          if(res.data.code==200){\r\n            this.$message({\r\n              type: 'success',\r\n              message: \"添加成功！\",\r\n            });\r\n          }else{\r\n            this.$message({\r\n              type: 'error',\r\n              message: res.data.message,\r\n            });\r\n          }\r\n        })\r\n      }).catch(fail=>{\r\n        this.$message({\r\n          type: 'info',\r\n          message: '已取消'\r\n        });          \r\n      });\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n.row{\r\n  height: 250px;\r\n}\r\n\r\n.descri