r\n          this.announcementList=res.data.result\r\n        }else{\r\n          this.$alert(res.data.message, '提示', {\r\n              confirmButtonText: '确定'\r\n          })\r\n        }\r\n      })\r\n      // 加载广告\r\n      this.$axios.get('/advertisement/getall')\r\n      .then(res=>{\r\n        if(res.data.code==200){\r\n          this.adList=res.data.result\r\n        }else{\r\n          this.$alert(res.data.message, '提示', {\r\n              confirmButtonText: '确定'\r\n          })\r\n        }\r\n      })\r\n      // 加载类型\r\n      this.$axios.get('/type/getall')\r\n      .then(res=>{\r\n        if(res.data.code==200){\r\n          this.typeList=res.data.result\r\n        }else{\r\n          this.$alert(res.data.message, '提示', {\r\n              confirmButtonText: '确定'\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.container{\r\n  margin: 30px 40px;\r\n}\r\n\r\n.top{\r\n  display: block;\r\n  height: 300px;\r\n  width: 100%;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.type{\r\n  width: 25%;\r\n  height: 100%;\r\n  float: left;\r\n}\r\n\r\n.typein{\r\n  margin-right: 20px;\r\n  height: 100%;\r\n  background-color: red;\r\n}\r\n\r\n.ad{\r\n  width: 50%;\r\n  height: 100%;\r\n  float: left;\r\n}\r\n\r\n.info{\r\n  width: 25%;\r\n  height: 100%;\r\n  float: left;\r\n}\r\n\r\n.infoin{\r\n  margin-left: 20px;\r\n  height: 100%;\r\n}\r\n\r\n.main{\r\n  display: block;\r\n}\r\n</style>"],"mappings":"AA6BA,OAAAA,aAAA;AACA,OAAAC,iBAAA;AACA,OAAAC,gBAAA;AACA,OAAAC,QAAA;AACA;EACAC,KAAA;IACA;MACAC,aAAA;MACAC,MAAA;MACAC,gBAAA;MACAC,QAAA;IACA;EACA;EACAC,UAAA;IACAT,aAAA;IACAC,iBAAA;IACAC,gBAAA;IACAC;EACA;EACAO,QAAA;IACA,KAAAC,QAAA;EACA;EACAC,OAAA;IACAD,SAAA;MACA;MACA,KAAAE,MAAA,CAAAC,GAAA,0BACAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAZ,IAAA,CAAAa,IAAA;UACA,KAAAZ,aAAA,GAAAW,GAAA,CAAAZ,IAAA,CAAAc,MAAA;QACA;UACA,KAAAC,MAAA,CAAAH,GAAA,CAAAZ,IAAA,CAAAgB,OAAA;YACAC,iBAAA;UACA;QACA;MACA;MACA;MACA,KAAAR,MAAA,CAAAC,GAAA,yBACAC,IAAA,CAAAC,GAAA;QACA