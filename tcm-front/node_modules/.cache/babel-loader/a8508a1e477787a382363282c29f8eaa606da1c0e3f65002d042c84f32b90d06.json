l-table-column\r\n          label=\"总价\"\r\n          width=\"100px\"\r\n          :show-overflow-tooltip=true>\r\n          <template slot-scope=\"scope\">\r\n            {{scope.row.price*scope.row.discount*scope.row.number}}\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"primary\"\r\n              @click=\"handleOrder(scope.$index, scope.row)\">提交订单</el-button>\r\n\r\n            <el-button\r\n              size=\"mini\"\r\n              @click=\"handleNumber(scope.$index, scope.row)\">修改数量</el-button>\r\n\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"info\"\r\n              @click=\"handleDetails(scope.$index, scope.row)\">商品详情</el-button>\r\n          </template>\r\n        </el-table-column>\r\n\r\n      </el-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      srcList:['']\r\n    }\r\n  },\r\n  created(){\r\n    this.loadData()\r\n  },\r\n  methods: {\r\n      //加载信息\r\n      loadData(){\r\n        this.$axios.get('/cart/getall/'+this.$store.state.user.id)\r\n        .then(res=>{\r\n          if(res.data.code==200){\r\n            this.tableData=res.data.result\r\n          }else{\r\n            this.$alert(res.data.message, '提示', {\r\n                confirmButtonText: '确定'\r\n            })\r\n          }\r\n        })\r\n      },\r\n      //提交订单\r\n      handleAdd(index, row) {\r\n        this.$confirm('是否将该商品加入购物车?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(res=>{\r\n          this.$axios.post('/cart/insert',{\r\n            cid:row.id,\r\n            uid:this.$store.state.user.id\r\n          }).then(res=>{\r\n            if(res.data.code==200){\r\n              this.$message({\r\n                type: 'success',\r\n                message: \"添加成功！\",\r\n              });\r\n            }else{\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        }).catch(fail=>{\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消'\r\n          });          \r\n        });\r\n      },\r\n      //详情\r\n      handleDetails(index, row) {\r\n        this.$router.push({\r\n          name:'commoditydetails',\r\n          query:{\r\n            data:JSON.stringify(row)\r\n          }\r\n        })\r\n      },\r\n      //大图\r\n      bigImg(url) {\r\n        this.srcList[0] = url;\r\n      },\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.table{\r\n  margin:20px;\r\n}\r\n\r\n.btn{\r\n  margin: 0px;\r\n  margin-bottom: 20px;\r\n  margin-right: 20px;\r\n  float: right;\r\n}\r\n\r\n</style>"],"mappings":";AA+FA;EACAA,KAAA;IACA;MACAC,SAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,QAAA;EACA;EACAC,OAAA;IACA;IACAD,SAAA;MACA,KAAAE,MAAA,CAAAC,GAAA,wBAAAC,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAC,EAAA,EACAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAb,IAAA,CAAAc,IAAA;UACA,KAAAb,SAAA,GAAAY,GAAA,CAAAb,IAAA,CAAAe,MAAA;QACA;UACA,KAAAC,MAAA,CAAAH,GAAA,CAAAb,IAAA,CAAAiB,OAAA;YACAC,iBAAA;UACA;QACA;MACA;IACA;IACA;IACAC,UAAAC,KAAA,EAAAC,GAAA;MACA,KAAAC,QAAA;QACAJ,iBAAA;QACAK,gBAAA;QACAC,IAAA;MACA,GAAAZ,IAAA,CAAAC,GAAA;QACA,KAAAP,MAAA,CAAAmB,IAAA;UACAC,GAAA,EAAAL,GAAA,CAAAV,EAAA;UACAgB,GAAA,OAAAnB,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAC;QACA,GAAAC,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAb,IAAA,CAAAc,IAAA;YACA,KAAAc,QAAA;cACAJ,IAAA;cACAP,OAAA;YACA;UACA;YACA,KAAAW,QAAA;cACAJ,IAAA;cACAP,OAAA,EAAAJ,GAAA,CAAAb,IAAA,CAAAiB;YACA;UACA;QACA;MACA,GAAAY,KAAA,CAAAC,IAAA;QACA,KAAAF,QAAA;UACAJ,IAAA;UACAP,OAAA;QACA;MACA;IACA;IACA;IACAc,cAAAX,KAAA,EAAAC,GAAA;MACA,KAAAW,OAAA,CAAAC,IAAA;QACAC,IAAA;QACAC,KAAA;UACAnC,IAAA,EAAAoC,IAAA,CAAAC,SAAA,CAAAhB,GAAA;QA