{"ast":null,"code":"export default {\n  data() {\n    return {\n      ruleForm: {},\n      rules: {\n        id: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        description: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        picturepath: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }]\n      },\n      types: []\n    };\n  },\n  created() {\n    this.ruleForm = JSON.parse(this.$route.query.data);\n  },\n  methods: {\n    goBack() {\n      this.$router.back();\n    },\n    //提交\n    submitForm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          this.$axios.put('/commodity/update', {\n            id: this.ruleForm.id,\n            description: this.ruleForm.description,\n            picturepath: this.ruleForm.picturepath\n          }).then(res => {\n            if (res.data.code == 200) {\n              this.$message({\n                type: 'success',\n                message: \"更新成功！\"\n              });\n            } else {\n              this.$message({\n                type: 'error',\n                message: res.data.message\n              });\n            }\n          });\n        } else {\n          this.$message({\n            type: 'error',\n            message: \"请检查输入信息！\"\n          });\n        }\n      });\n    },\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n    },\n    //图片上传\n    handleAvatarSuccess(res, file) {\n      this.ruleForm.picturepath = res.result;\n    },\n    beforeAvatarUpload(file) {\n      const isJPG = file.type === 'image/jpeg';\n      const isLt2M = file.size / 1024 / 1024 < 4;\n      if (!isJPG) {\n        this.$message.error('上传头像图片只能是 JPG 格式!');\n      }\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 4MB!');\n      }\n      return isJPG && isLt2M;\n    }\n  }\n};","map":{"version":3,"names":["data","ruleForm","rules","id","required","message","trigger","description","picturepath","types","created","JSON","parse","$route","query","methods","goBack","$router","back","submitForm","formName","$refs","validate","valid","$axios","put","then","res","code","$message","type","resetForm","resetFields","handleAvatarSuccess","file","result","beforeAvatarUpload","isJPG","isLt2M","size","error"],"sources":["src/views/Operator/Advertisement/Update/index.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-page-header @back=\"goBack\" content=\"更新广告信息\" style=\"margin:20px;\"></el-page-header>\r\n\r\n    <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\" class=\"demo-ruleForm\">\r\n\r\n      <el-form-item prop=\"picturepath\">\r\n        <el-upload\r\n          class=\"avatar-uploader\"\r\n          action=\"http://localhost:8081/upload/advertisement\"\r\n          :show-file-list=\"false\"\r\n          :on-success=\"handleAvatarSuccess\"\r\n          :before-upload=\"beforeAvatarUpload\"\r\n          style=\"margin:50px;\">\r\n          <img v-if=\"ruleForm.picturepath\" :src=\"`http://localhost:8081/`+ruleForm.picturepath\" class=\"avatar\">\r\n          <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n        </el-upload>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"ID\" prop=\"id\">\r\n        <el-input v-model=\"ruleForm.id\" disabled></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"描述\" prop=\"description\">\r\n        <el-input v-model=\"ruleForm.description\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">提交</el-button>\r\n        <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\r\n      </el-form-item>\r\n\r\n    </el-form>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      ruleForm: {\r\n\r\n      },\r\n      rules: {\r\n        id: [{required: true, message: '', trigger: 'blur'}],\r\n        description: [{required: true, message: '', trigger: 'blur'}],\r\n        picturepath: [{required: true, message: '', trigger: 'blur'}],\r\n      },\r\n      types:[]\r\n    };\r\n  },\r\n  created(){\r\n    this.ruleForm=JSON.parse(this.$route.query.data)\r\n  },\r\n  methods: {\r\n    goBack(){\r\n      this.$router.back()\r\n    },\r\n    //提交\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.$axios.put('/commodity/update',{\r\n            id:this.ruleForm.id,\r\n            description:this.ruleForm.description,\r\n            picturepath:this.ruleForm.picturepath,\r\n          }).then(res=>{\r\n            if(res.data.code==200){\r\n              this.$message({\r\n                type: 'success',\r\n                message: \"更新成功！\",\r\n              });\r\n            }else{\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: 'error',\r\n            message: \"请检查输入信息！\",\r\n          });\r\n        }\r\n      });\r\n    },\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n\r\n    //图片上传\r\n    handleAvatarSuccess(res, file) {\r\n      this.ruleForm.picturepath=res.result\r\n    },\r\n\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === 'image/jpeg';\r\n      const isLt2M = file.size / 1024 / 1024 < 4;\r\n\r\n      if (!isJPG) {\r\n        this.$message.error('上传头像图片只能是 JPG 格式!');\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error('上传头像图片大小不能超过 4MB!');\r\n      }\r\n      return isJPG && isLt2M;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.el-form{\r\n  margin: auto;\r\n  margin-top:20px;\r\n  width:50%;\r\n}\r\n\r\n.avatar-uploader .el-upload {\r\n 