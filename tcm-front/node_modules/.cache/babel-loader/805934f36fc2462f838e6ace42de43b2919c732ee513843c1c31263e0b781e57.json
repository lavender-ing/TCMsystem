{"ast":null,"code":"export default {\n  data() {\n    //价格大于0\n    var checkPrice = (rule, value, cb) => {\n      if (value > 0) {\n        //合法\n        return cb();\n      }\n      //不合法\n      cb(new Error('价格应大于等于0！'));\n    };\n\n    //折扣\n    var checkDiscount = (rule, value, cb) => {\n      if (value >= 0 && value <= 1) {\n        //合法\n        return cb();\n      }\n      //不合法\n      cb(new Error('折扣应在0~1之间！'));\n    };\n    return {\n      ruleForm: {},\n      rules: {\n        id: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        name: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        brand: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        price: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }, {\n          validator: checkPrice,\n          trigger: 'blur'\n        }],\n        discount: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }, {\n          validator: checkDiscount,\n          trigger: 'blur'\n        }],\n        type: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        description: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }],\n        picturepath: [{\n          required: true,\n          message: '',\n          trigger: 'blur'\n        }]\n      },\n      types: []\n    };\n  },\n  created() {\n    this.ruleForm = JSON.parse(this.$route.query.data);\n    this.loadData();\n  },\n  methods: {\n    goBack() {\n      this.$router.back();\n    },\n    //加载数据\n    loadData() {\n      this.$axios.get('/type/getall').then(res => {\n        if (res.data.code == 200) {\n          this.types = res.data.result;\n        } else {\n          this.$alert(res.data.message, '提示', {\n            confirmButtonText: '确定'\n          });\n        }\n      });\n    },\n    //提交\n    submitForm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          this.$axios.put('/commodity/update', {\n            id: this.ruleForm.id,\n            name: this.ruleForm.name,\n            brand: this.ruleForm.brand,\n            price: this.ruleForm.price,\n            discount: this.ruleForm.discount,\n            description: this.ruleForm.description,\n            picturepath: this.ruleForm.picturepath\n          }).then(res => {\n            if (res.data.code == 200) {\n              this.$message({\n                type: 'success',\n                message: \"更新成功！\"\n              });\n            } else {\n              this.$message({\n                type: 'error',\n                message: res.data.message\n              });\n            }\n          });\n        } else {\n          this.$message({\n            type: 'error',\n            message: \"请检查输入信息！\"\n          });\n        }\n      });\n    },\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n    },\n    //图片上传\n    handleAvatarSuccess(res, file) {\n      this.ruleForm.picturepath = res.result;\n    },\n    beforeAvatarUpload(file) {\n      const isJPG = file.type === 'image/jpeg';\n      const isLt2M = file.size / 1024 / 1024 < 2;\n      if (!isJPG) {\n        this.$message.error('上传头像图片只能是 JPG 格式!');\n      }\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 2MB!');\n      }\n      return isJPG && isLt2M;\n    }\n  }\n};","map":{"version":3,"names":["data","checkPrice","rule","value","cb","Error","checkDiscount","ruleForm","rules","id","required","message","trigger","name","brand","price","validator","discount","type","description","picturepath","types","created","JSON","parse","$route","query","loadData","methods","goBack","$router","back","$axios","get","then","res","code","result","$alert","confirmButtonText","submitForm","formName","$refs","validate","vali