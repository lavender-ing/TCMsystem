l-form-item>\r\n\r\n      <el-form-item label=\"描述\" prop=\"description\">\r\n        <el-input v-model=\"ruleForm.description\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">提交</el-button>\r\n        <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\r\n      </el-form-item>\r\n\r\n    </el-form>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n\r\n    //价格大于0\r\n    var checkPrice = (rule,value,cb) =>{\r\n      if(value>0){\r\n        //合法\r\n        return cb();\r\n      }\r\n      //不合法\r\n      cb(new Error('价格应大于等于0！'));\r\n    }\r\n\r\n    //折扣\r\n    var checkDiscount = (rule,value,cb) =>{\r\n      if(value>=0 && value<=1){\r\n        //合法\r\n        return cb();\r\n      }\r\n      //不合法\r\n      cb(new Error('折扣应在0~1之间！'));\r\n    }\r\n\r\n    return {\r\n      ruleForm: {\r\n\r\n      },\r\n      rules: {\r\n        id: [{required: true, message: '', trigger: 'blur'}],\r\n        name: [{required: true, message: '', trigger: 'blur'}],\r\n        brand: [{required: true, message: '', trigger: 'blur'}],\r\n        price: [{required: true, message: '', trigger: 'blur'},{validator: checkPrice, trigger: 'blur'}],\r\n        discount: [{required: true, message: '', trigger: 'blur'},{validator: checkDiscount, trigger: 'blur'}],\r\n        type: [{required: true, message: '', trigger: 'blur'}],\r\n        description: [{required: true, message: '', trigger: 'blur'}],\r\n        picturepath: [{required: true, message: '', trigger: 'blur'}],\r\n      },\r\n      types:[]\r\n    };\r\n  },\r\n  created(){\r\n    this.ruleForm=JSON.parse(this.$route.query.data)\r\n    this.loadData()\r\n  },\r\n  methods: {\r\n    goBack(){\r\n      this.$router.back()\r\n    },\r\n    //加载数据\r\n    loadData(){\r\n      this.$axios.get('/type/getall')\r\n        .then(res=>{\r\n          if(res.data.code==200){\r\n            this.types=res.data.result\r\n          }else{\r\n            this.$alert(res.data.message, '提示', {\r\n                confirmButtonText: '确定'\r\n            })\r\n          }\r\n        })\r\n    },\r\n    //提交\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.$axios.put('/commodity/update',{\r\n            id:this.ruleForm.id,\r\n            name:this.ruleForm.name,\r\n            brand:this.ruleForm.brand,\r\n            price:this.ruleForm.price,\r\n            discount:this.ruleForm.discount,\r\n            description:this.ruleForm.description,\r\n            picturepath:this.ruleForm.picturepath\r\n          }).then(res=>{\r\n            if(res.data.code==200){\r\n              this.$message({\r\n                type: 'success',\r\n                message: \"更新成功！\",\r\n              });\r\n            }else{\r\n              this.$message({\r\n                type: 'error',\r\n                message: res.data.message,\r\n              });\r\n            }\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: 'error',\r\n            message: \"请检查输入信息！\",\r\n          });\r\n        }\r\n      });\r\n    },\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n\r\n    //图片上传\r\n    handleAvatarSuccess(res, file) {\r\n      this.ruleForm.picturepath=res.result\r\n    },\r\n\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === 'image/jpeg';\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n\r\n      if (!isJPG) {\r\n        this.$message.error('上传头像图片只能是 JPG 格式!');\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error('上传头像图片大小不能超过 2MB!');\r\n      }\r\n      return isJPG && isLt2M;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.el-form{\r\n  margin: auto;\r\n  margin-top:20px;\r\n  width:50%;\r\n}\r\n\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409EFF;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n</style>"],"mappings":"AA+DA;EACAA,KAAA;IAEA;IACA,IAAAC,UAAA,GAAAA,CAAAC,IAAA,EAAAC,KAAA,EAAAC,EAAA;MACA,IAAAD,KAAA;QACA;QACA,OAAAC,EAAA;MACA;MACA;MACAA,EAAA,KAAAC,KAAA;IACA;;IAEA;IACA,IAAAC,aAAA,GAAAA,CAAAJ,IAAA,EAAAC,KAAA,EAAAC,EAAA;MACA,IAAAD,KAAA,SAAAA,KAAA;QACA;QACA,OAAAC,EAAA;MACA;MACA;MACAA,EAAA,KAAAC,KAAA;IACA;IAEA;MACAE,QAAA,GAEA;MACAC,KAAA;QACAC,EAAA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAC,IAAA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAE,KAAA;UAAAJ,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAG,KAAA;UAAAL,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;UAAAI,SAAA,EAAAf,UAAA;UAAAW,OAAA;QAAA;QACAK,QAAA;UAAAP,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;UAAAI,SAAA,EAAAV,aAAA;UAAAM,OAAA;QAAA;QACAM,IAAA;UAAAR,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAO,WAAA;UAAAT,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAQ,WAAA;UAAAV,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;MACAS,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAf,QAAA,GAAAgB,IAAA,CAAAC,KAAA,MAAAC,MAAA,CAAAC,KAAA,CAAA1B,IAAA;IACA,KAAA2B,QAAA;EACA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACA;IACAJ,SAAA;MACA,KAAAK,MAAA,CAAAC,GAAA,iBACAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAnC,IAAA,CAAAoC,IAAA;UACA,KAAAf,KAAA,GAAAc,GAAA,CAAAnC,IAAA,CAAAqC,MAAA;QACA;UACA,KAAAC,MAAA,CAAAH,GAAA,CAAAnC,IAAA,CAAAW,OAAA;YACA4B,iBAAA;UACA;QACA;MACA;IACA;IACA;IACAC,WAAAC,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAZ,MAAA,CAAAa,GAAA;YACApC,EAAA,OAAAF,QAAA,CAAAE,EAAA;YACAI,IAAA,OAAAN,QAAA,CAAAM,IAAA;YACAC,KAAA,OAAAP,QAAA,CAAAO,KAAA;YACAC,KAAA,OAAAR,QAAA,CAAAQ,KAAA;YACAE,QAAA,OAAAV,QAAA,CAAAU,QAAA;YACAE,WAAA,OAAAZ,QAAA,CAAAY,WAAA;YACAC,WAAA,OAAAb,QAAA,CAAAa;UACA,GAAAc,IAAA,CAAAC,GA